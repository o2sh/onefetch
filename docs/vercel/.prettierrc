{
  "useTabs": false,
  "tabWidth": 2,
  "semi": true,
  "singleQuote": true,
  "trailingComma": "es5",
  "bracketSameLine": true,
  "singleAttributePerLine": false,
  "quoteProps": "consistent",
  "plugins": ["prettier-plugin-svelte"],
  "svelteSortOrder": "scripts-options-markup-styles",
  "svelteIndentScriptAndStyle": true
}
